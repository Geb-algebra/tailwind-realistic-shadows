    "remix-flat-routes": "^0.8.5",
    "tailwindcss": "^4.0.12",
    "typescript": "^5.8.2",
    "vite": "^6.2.1",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.8",
    "wrangler": "^3.114.0"
